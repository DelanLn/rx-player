#!/bin/bash

# Clean-up builds
# =================
#
# This script removes the directories automatically generated when doing a
# modular build.
#
# You can call it at any time, it will not update any staged file.

set -e
rm -rf dist/_esm5.processed
rm -rf dist/_esm5.raw

rm -rf features
rm -rf minimal
rm -rf experimental
rm -rf tools
